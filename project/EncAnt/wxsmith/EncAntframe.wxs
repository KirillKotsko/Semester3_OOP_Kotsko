<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="EncAntFrame">
		<title>EncAnt</title>
		<icon>images\Entypo_2712(0)_1024.ico</icon>
		<size>497,417</size>
		<bg>wxSYS_COLOUR_SCROLLBAR</bg>
		<handler function="OnClose" entry="EVT_CLOSE" />
		<object class="wxFilePickerCtrl" name="ID_FILEPICKERCTRL1" variable="FilePickerCtrl1" member="yes">
			<message></message>
			<wildcard></wildcard>
			<pos>112,25</pos>
			<size>352,23</size>
			<handler function="OnFilePickerCtrl1FileChanged" entry="EVT_FILEPICKER_CHANGED" />
		</object>
		<object class="wxButton" name="ID_BUTTON1" variable="Button1" member="yes">
			<label>Encrypt</label>
			<pos>40,104</pos>
			<handler function="OnButton1Click" entry="EVT_BUTTON" />
		</object>
		<object class="wxButton" name="ID_BUTTON2" variable="Button2" member="yes">
			<label>Decrypt</label>
			<pos>200,104</pos>
			<handler function="OnButton2Click2" entry="EVT_BUTTON" />
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
			<label>Select file:</label>
			<pos>32,25</pos>
			<font>
				<size>14</size>
				<style>normal</style>
				<weight>normal</weight>
				<underlined>0</underlined>
				<family>swiss</family>
				<face>Times New Roman</face>
			</font>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
			<label>Enter the key:</label>
			<pos>32,65</pos>
			<font>
				<size>14</size>
				<style>normal</style>
				<weight>normal</weight>
				<underlined>0</underlined>
				<family>swiss</family>
				<face>Times New Roman</face>
			</font>
		</object>
		<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="TextCtrl1" member="yes">
			<pos>136,65</pos>
			<size>328,21</size>
			<handler function="OnTextCtrl1Text2" entry="EVT_TEXT" />
		</object>
		<object class="wxStaticBitmap" name="ID_STATICBITMAP1" variable="StaticBitmap1" member="yes">
			<bitmap>images\Entypo_2712(0)_1024.png</bitmap>
			<pos>16,144</pos>
			<size>256,256</size>
			<style>0</style>
		</object>
		<object class="wxStaticBitmap" name="ID_STATICBITMAP2" variable="StaticBitmap2" member="yes">
			<bitmap>images\padlock.png</bitmap>
			<pos>280,128</pos>
			<size>199,258</size>
			<style>0</style>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;Menu</label>
				<object class="wxMenuItem" name="idInstuction" variable="MenuItem2" member="yes">
					<label>Instuction</label>
					<accel>F2</accel>
					<handler function="OnInstruction" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
		</object>
	</object>
</wxsmith>
